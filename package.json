{
  "name": "eosjs2",
  "version": "0.0.13",
  "description": "Talk to eos API",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "jest",
    "build": "tsc -p ./tsconfig.node.json && cp src/ripemd.js dist && babel dist --out-dir dist",
    "build-web": "webpack"
  },
  "author": "block.one",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EOSIO/eosjs2.git"
  },
  "dependencies": {
    "@types/jest": "^23.3.1",
    "babel-polyfill": "^6.26.0",
    "eosjs-ecc": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^10.3.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "jest": "^23.5.0",
    "jest-fetch-mock": "^1.6.5",
    "json-loader": "^0.5.7",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.3.1",
    "typescript": "^2.9.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  }
}
